{
    "name": "drupal-composer/drupal-project",
    "description": "Project template for Drupal 8 projects with composer",
    "type": "project",
    "license": "GPL-2.0+",
    "authors": [
        {
            "name": "",
            "role": ""
        }
    ],
    "repositories": {
        "drupal": {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        },
        "leaflet": {
            "type": "package",
            "package": {
                "name": "leaflet/leaflet",
                "version": "v0.7.7",
                "type": "drupal-library",
                "dist": {
                    "url": "https://github.com/Leaflet/Leaflet/archive/v0.7.7.zip",
                    "type": "zip"
                }
            }
        },
        "lang_switcher": {
            "type": "vcs",
            "url": "https://git.drupal.org/sandbox/mhavelant/2831414.git"
        }
    },
    "require": {
        "composer/installers": "^1.0.20",
        "drupal-composer/drupal-scaffold": "^2.0.1",
        "cweagans/composer-patches": "~1.0",
        "drupal/core": "~8.0",
        "drush/drush": "~8.0",
        "wikimedia/composer-merge-plugin": "~1.3",
        "aws/aws-sdk-php": "~3.0",
        "drupal/parade": "1.x-dev",
        "drupal/cloudfront_purger": "1.x-dev",
        "drupal/metatag": "^1.0",
        "drupal/publishcontent": "~1.0",
        "drupal/video_embed_field": "~1.3",
        "drupal/contact_block": "~1.2",
        "drupal/entity_embed": "~1.0",
        "drupal/addtoany": "^1.7",
        "drupal/content_entity_language_switcher": "dev-8.x-1.x",
        "drupal/classy_paragraphs": "^1.0",
        "drupal/node_clone": "1.x-dev",
        "drupal/lang_dropdown": "2.x-dev",
        "drupal/diff": "^1.0",
        "drupal/recaptcha": "^2.2",
        "drupal/config_readonly": "^1.0",
        "drupal/scheduled_updates": "1.x-dev",
        "drupal/seckit": "^1.0@alpha"
    },
    "require-dev": {
        "behat/mink": "~1.7",
        "behat/mink-goutte-driver": "~1.2",
        "jcalderonzumba/gastonjs": "~1.0.2",
        "jcalderonzumba/mink-phantomjs-driver": "~0.3.1",
        "mikey179/vfsStream": "~1.2",
        "phpunit/phpunit": "~4.8",
        "symfony/css-selector": "~2.8",
        "drupal/console": "~1.0",
        "drupal/devel": "1.x-dev",
        "drupal/examples": "1.x-dev",
        "drupal/admin_toolbar": "^1.18",
        "drupal/adminimal_admin_toolbar": "^1.3",
        "drupal/toolbar_anti_flicker": "^2.5"
    },
    "conflict": {
        "drupal/drupal": "*"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "classmap": [
            "scripts/composer/ScriptHandler.php"
        ]
    },
    "scripts": {
        "drupal-scaffold": "DrupalComposer\\DrupalScaffold\\Plugin::scaffold",
        "pre-install-cmd": [
            "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
        ],
        "pre-update-cmd": [
            "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
        ],
        "post-install-cmd": [
            "DrupalProject\\composer\\ScriptHandler::createRequiredFiles"
        ],
        "post-update-cmd": [
            "DrupalProject\\composer\\ScriptHandler::createRequiredFiles"
        ]
    },
    "extra": {
        "installer-paths": {
            "web/core": ["type:drupal-core"],
            "web/libraries/{$name}": ["type:drupal-library"],
            "web/modules/contrib/{$name}": ["type:drupal-module"],
            "web/profiles/contrib/{$name}": ["type:drupal-profile"],
            "web/themes/contrib/{$name}": ["type:drupal-theme"],
            "drush/contrib/{$name}": ["type:drupal-drush"]
        },
        "patches": {
            "drupal/node_clone": {
                "Use tab instead of action link for cloning.": "https://www.drupal.org/files/issues/use_a_tab_instead_of-2824227-2.patch"
            },
            "drupal/entity_reference_revisions": {
                "Deletion of referenced entities on host delete, without checking usage/another reference.": "https://www.drupal.org/files/issues/deletion_of_used_referenced_entity-2852522-4.patch"
            },
            "drupal/scheduled_updates": {
                "Scheduled Updates does not work with Content Moderation.": "https://www.drupal.org/files/issues/scheduled_updates_does-2821916-7.patch"
            },
            "drupal/geocoder": {
                "Fix config schema.": "https://www.drupal.org/files/issues/2824802-geocoder-schema-fix-2.patch"
            },
            "drupal/geocoder_autocomplete": {
                "Add missing schema.": "https://www.drupal.org/files/issues/missing-schema-2858115-2.patch"
            },
            "drupal/purge_queuer_url": {
                "only queue anonymous requests": "https://www.drupal.org/files/issues/purge_queuer_url-prevent-queueing_maxage_less_than_0-2847828-1.patch"
            },
            "drupal/cloudfront_purger": {
                "watchdog placeholder syntax fix": "https://www.drupal.org/files/issues/cloudfront_purger-watchdog_placeholder_syntax_fix-2863548-1.patch"
            }
        }
    }
}
