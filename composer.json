{
    "name": "drupal-composer/drupal-project",
    "description": "Project template for Drupal 8 projects with composer",
    "type": "project",
    "license": "GPL-2.0+",
    "authors": [
        {
            "name": "",
            "role": ""
        }
    ],
    "repositories": {
        "drupal": {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        },
        "leaflet": {
            "type": "package",
            "package": {
                "name": "leaflet/leaflet",
                "version": "v0.7.7",
                "type": "drupal-library",
                "dist": {
                    "url": "https://github.com/Leaflet/Leaflet/archive/v0.7.7.zip",
                    "type": "zip"
                }
            }
        },
        "lang_switcher": {
            "type": "vcs",
            "url": "https://git.drupal.org/sandbox/mhavelant/2831414.git"
        },
        "drupal/client_support": {
            "type": "vcs",
            "url": "https://github.com/brainsum/client_support"
        },
        "drupal/parade_content_lister": {
            "type": "vcs",
            "url": "https://github.com/brainsum/parade_content_lister"
        },
        "parade": {
            "type": "vcs",
            "url": "git@github.com:brainsum/parade.git"
        }
    },
    "require": {
        "composer/installers": "^1.2",
        "drupal-composer/drupal-scaffold": "^2.2",
        "cweagans/composer-patches": "^1.6",
        "drupal/core": "^8.5",
        "drush/drush": "~8.1",
        "aws/aws-sdk-php": "~3.0",
        "drupal/cloudfront_purger": "~1.1",
        "drupal/metatag": "^1.0",
        "drupal/video_embed_field": "~1.3",
        "drupal/contact_block": "~1.2",
        "drupal/entity_embed": "~1.0",
        "drupal/addtoany": "^1.7",
        "drupal/content_entity_language_switcher": "dev-8.x-1.x",
        "drupal/node_clone": "1.x-dev",
        "drupal/lang_dropdown": "~2.0",
        "drupal/diff": "^1.0",
        "drupal/recaptcha": "^2.2",
        "drupal/config_readonly": "^1.0",
        "drupal/seckit": "^1.0",
        "drupal/redirect": "^1.0",
        "drupal/maxlength": "^1.0",
        "drupal/abtestui": "^1.0",
        "drupal/parade": "dev-8.x-2.x-testing",
        "drupal/allowed_formats": "^1.1",
        "drupal/public_preview": "^1.0",
        "drupal-composer/drupal-security-advisories": "8.x-dev",
        "webflo/drupal-finder": "^1.0.0",
        "webmozart/path-util": "^2.3",
        "drupal/facebook_pixel": "^1.0",
        "drupal/imagemagick": "~1.0",
        "drupal/imageapi_optimize": "^2.0@alpha",
        "drupal/google_tag": "1.x-dev",
        "drupal/filefield_sources_jsonapi": "1.x-dev",
        "drupal/override_node_options": "^2.3",
        "drupal/view_unpublished": "dev-1.x",
        "drupal/client_support": "dev-master",
        "drupal/parade_content_lister": "dev-master",
        "drupal/workbench_moderation": "^1.2",
        "drupal/scheduled_updates": "1.x-dev",
        "drupal/geysir": "1.x-dev",
        "drupal/paragraphs_edit": "^2.0@alpha"
    },
    "require-dev": {
        "behat/mink": "~1.7",
        "behat/mink-goutte-driver": "~1.2",
        "jcalderonzumba/gastonjs": "~1.0.2",
        "jcalderonzumba/mink-phantomjs-driver": "~0.3.1",
        "mikey179/vfsStream": "~1.2",
        "phpunit/phpunit": ">=4.8.28 <5",
        "symfony/css-selector": "~2.8",
        "drupal/console": "^1.0.1",
        "drupal/devel": "~1.0",
        "drupal/examples": "1.x-dev",
        "drupal/admin_toolbar": "^1.18",
        "drupal/adminimal_admin_toolbar": "^1.3",
        "drupal/toolbar_anti_flicker": "^2.5",
        "mikey179/vfsstream": "~1.2"
    },
    "conflict": {
        "drupal/drupal": "*"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "classmap": [
            "scripts/composer/ScriptHandler.php"
        ]
    },
    "scripts": {
        "drupal-scaffold": "DrupalComposer\\DrupalScaffold\\Plugin::scaffold",
        "pre-install-cmd": [
            "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
        ],
        "pre-update-cmd": [
            "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
        ],
        "post-install-cmd": [
            "DrupalProject\\composer\\ScriptHandler::createRequiredFiles"
        ],
        "post-update-cmd": [
            "DrupalProject\\composer\\ScriptHandler::createRequiredFiles"
        ]
    },
    "extra": {
        "installer-paths": {
            "web/core": ["type:drupal-core"],
            "web/libraries/{$name}": ["type:drupal-library"],
            "web/modules/contrib/{$name}": ["type:drupal-module"],
            "web/profiles/contrib/{$name}": ["type:drupal-profile"],
            "web/themes/contrib/{$name}": ["type:drupal-theme"],
            "drush/contrib/{$name}": ["type:drupal-drush"]
        },
        "patches": {
            "drupal/node_clone": {
                "Use tab instead of action link for cloning.": "https://www.drupal.org/files/issues/use_a_tab_instead_of-2824227-2.patch"
            },
            "drupal/entity_reference_revisions": {
                "Deletion of referenced entities on host delete, without checking usage/another reference.": "patches/deletion_of_used_referenced_entity-2852522-4.patch"
            },
            "drupal/scheduled_updates": {
                "Cron Updates not runned by User #1": "https://www.drupal.org/files/issues/2793489-20.patch",
                "Fix revisions": "https://www.drupal.org/files/issues/scheduled_updates-fix-all-revisions-queries-2872239-36.patch"
            },
            "drupal/geocoder_autocomplete": {
                "Add missing schema.": "https://www.drupal.org/files/issues/missing-schema-2858115-2.patch"
            },
            "drupal/core": {
                "remove X-Frame-Options header from core": "patches/remove-x-frame-options-header-from-core.patch",
                "Multilingual fieldset title fix.": "https://www.drupal.org/files/issues/title_for_details_form-2652850-30.patch",
                "Link to Entity get invalid language link in views": "https://www.drupal.org/files/issues/reroll_patch_for_issue_2862511.patch",
                "Paragraphs_edit vs quick edit": "patches/quickedit_2922324_custom.patch",
                "TODO: fix me proper way, removes paddings on quick edit": "patches/drupal-8-custom-patch-no-padding-inplace-edit.patch"
            },
            "drupal/classy_paragraphs": {
                "Enable chose and order for the classy paragraphs": "https://www.drupal.org/files/issues/choose_and_order-2830403-11.patch"
            },
            "drupal/workbench_moderation": {
                "Do not force the creation of new revisions": "https://www.drupal.org/files/issues/do_not_force_new_revisions-2768977-6.patch"
            },
            "drupal/filefield_sources_jsonapi": {
                "Get preview image via curl" : "patches/RetrieveRemoteFileWithCurl.patch"
            },
            "drupal/geysir": {
                "Allow to reorder paragraphs": "https://www.drupal.org/files/issues/geysir-reordering-2900784-22-D8.patch",
                "Use ReplaceCommand instead of HtmlCommand": "patches/geysir-replace-custom-fixed-in-feature.patch"
            },
            "drupal/field_group": {
                "Field groups are not compatible with field layout": "https://www.drupal.org/files/issues/field-layout-support-2878359-36.patch"
            },
            "drupal/paragraphs": {
                "Fix support for field_group module in combination with field_layout module patch": "https://www.drupal.org/files/issues/2907094_7_field_group_support.patch"
            },
            "drupal/parade": {
                "Parade aggregated leaflet vs leaflet-8.x-1.0-beta3": "patches/parade-aggregated_leaflet_map.leaflet-beta3.patch"
            }
        }
    },
    "config": {
        "discard-changes": true
    }
}
